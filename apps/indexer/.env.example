# Indexer (.env.example)
# Copy to .env and fill in values

# Ethereum RPC
RPC_HTTP_URL=
RPC_WS_URL=

# ERC-8004 Identity Registry (address)
IDENTITY_REGISTRY=

# Indexing behavior
CONFIRMATIONS=12
DB_PATH=./data/registry.db
START_BLOCK=0
LOGS_CHUNK_SIZE=10
BACKFILL_MODE=logs # logs | ids

# Downstream API (identity-service)
IDENTITY_API_URL=http://localhost:4000