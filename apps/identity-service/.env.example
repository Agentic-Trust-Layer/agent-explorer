# Identity Service (.env.example)
# Copy to .env and fill in values

PORT=4000


# Web3.Storage (optional but recommended)
WEB3_STORAGE_EMAIL=
WEB3_STORAGE_SPACE_DID=

# CORS
CLIENT_ORIGIN=http://localhost:3000

# DB path (reads indexer DB by default)
DB_PATH=../indexer/data/registry.db

# Base API URL (for internal callbacks)
API_URL=http://localhost:4000
