## Where this repo is located on the VM (optional).
## If omitted, the runner assumes it is started from the repo root.
# REPO_ROOT=/srv/agent-explorer

## Port the runner listens on
PORT=8787

## Sync pipeline secrets (these live ONLY on the VM/container)
GRAPHDB_BASE_URL=https://graphdb.agentkg.io
GRAPHDB_REPOSITORY=agentkg
GRAPHDB_USERNAME=
GRAPHDB_PASSWORD=

# Subgraph endpoints used by the sync pipeline (set the ones you run).
ETH_MAINNET_GRAPHQL_URL=
ETH_SEPOLIA_GRAPHQL_URL=
BASE_SEPOLIA_GRAPHQL_URL=
LINEA_MAINNET_GRAPHQL_URL=
LINEA_SEPOLIA_GRAPHQL_URL=

# ENS subdomain sync (runs as part of sync:agent-pipeline)
ENS_MAINNET_GRAPHQL_URL=
ENS_SEPOLIA_GRAPHQL_URL=
ETH_MAINNET_RPC_HTTP_URL=
ETH_SEPOLIA_RPC_HTTP_URL=

# ENS org-name configuration (used to choose the parent ENS name to enumerate; ".eth" is appended if missing)
NEXT_PUBLIC_AGENTIC_TRUST_ENS_ORG_NAME=8004-agent
NEXT_PUBLIC_AGENTIC_TRUST_ENS_ORG_NAME_SEPOLIA=8004-agent
NEXT_PUBLIC_AGENTIC_TRUST_ENS_ORG_NAME_LINEA=8004-agent
NEXT_PUBLIC_AGENTIC_TRUST_ENS_ORG_NAME_BASE_SEPOLIA=agnt

