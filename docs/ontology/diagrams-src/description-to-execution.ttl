@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix core: <https://agentictrust.io/ontology/core#> .

core:Diagram_DescriptionToExecution a owl:Ontology ;
  rdfs:label "DescriptionToExecution" .

core:TrustDescription a owl:Class .

core:TaskExecution a owl:Class .
core:SkillInvocation a owl:Class .
core:AgentSkillClassification a owl:Class .
core:TaskType a owl:Class .
core:IntentType a owl:Class .

core:hadPlan a owl:ObjectProperty ;
  rdfs:domain core:TaskExecution ;
  rdfs:range core:TrustDescription .

core:hasInvocation a owl:ObjectProperty ;
  rdfs:domain core:TaskExecution ;
  rdfs:range core:SkillInvocation .

core:invokesSkill a owl:ObjectProperty ;
  rdfs:domain core:SkillInvocation ;
  rdfs:range core:AgentSkillClassification .

core:enablesTaskType a owl:ObjectProperty ;
  rdfs:domain core:AgentSkillClassification ;
  rdfs:range core:TaskType .

core:supportsIntentType a owl:ObjectProperty ;
  rdfs:domain core:AgentSkillClassification ;
  rdfs:range core:IntentType .

core:mapsToTaskType a owl:ObjectProperty ;
  rdfs:domain core:IntentType ;
  rdfs:range core:TaskType .


