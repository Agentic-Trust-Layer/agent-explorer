@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix core: <https://agentictrust.io/ontology/core#> .
@prefix erc8004: <https://agentictrust.io/ontology/erc8004#> .
@prefix erc8092: <https://agentictrust.io/ontology/erc8092#> .

core:Diagram_DiscoveryTrustSignals a owl:Ontology ;
  rdfs:label "DiscoveryTrustSignals" .

core:AIAgent a owl:Class .
core:TrustAssertion a owl:Class .
core:VerificationTrustAssertion a owl:Class ; rdfs:subClassOf core:TrustAssertion .
core:ReputationTrustAssertion a owl:Class ; rdfs:subClassOf core:TrustAssertion .
core:RelationshipTrustAssertion a owl:Class ; rdfs:subClassOf core:TrustAssertion .

erc8004:ValidationResponse a owl:Class ; rdfs:subClassOf core:VerificationTrustAssertion .
erc8004:Feedback a owl:Class ; rdfs:subClassOf core:ReputationTrustAssertion .
erc8092:RelationshipAssertionERC8092 a owl:Class ; rdfs:subClassOf core:RelationshipTrustAssertion .

erc8004:hasValidation a owl:ObjectProperty ;
  rdfs:domain core:AIAgent ;
  rdfs:range erc8004:ValidationResponse .

erc8004:hasFeedback a owl:ObjectProperty ;
  rdfs:domain core:AIAgent ;
  rdfs:range erc8004:Feedback .

core:hasTrustAssertion a owl:ObjectProperty ;
  rdfs:domain core:AIAgent ;
  rdfs:range core:TrustAssertion .


