@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix core: <https://agentictrust.io/ontology/core#> .
@prefix erc8004: <https://agentictrust.io/ontology/erc8004#> .

erc8004:Diagram_Registration a owl:Ontology ;
  rdfs:label "ERC8004RegistrationDescriptor" .

prov:Entity a owl:Class .
prov:SoftwareAgent a owl:Class .

core:AIAgent a owl:Class ; rdfs:subClassOf prov:SoftwareAgent .

erc8004:AgentIdentity8004 a owl:Class ; rdfs:subClassOf prov:Entity .
erc8004:AgentRegistration8004 a owl:Class ; rdfs:subClassOf prov:Entity .

core:Endpoint a owl:Class ; rdfs:subClassOf prov:Entity .
core:AgentSkill a owl:Class ; rdfs:subClassOf prov:Entity .
core:AgentSkillClassification a owl:Class ; rdfs:subClassOf prov:Entity .
core:AgentDomain a owl:Class ; rdfs:subClassOf prov:Entity .
core:AgentDomainClassification a owl:Class ; rdfs:subClassOf prov:Entity .

core:hasIdentity a owl:ObjectProperty ;
  rdfs:domain core:AIAgent ;
  rdfs:range erc8004:AgentIdentity8004 .

core:hasDescriptor a owl:ObjectProperty ;
  rdfs:domain erc8004:AgentIdentity8004 ;
  rdfs:range erc8004:AgentRegistration8004 .

core:hasEndpoint a owl:ObjectProperty ;
  rdfs:domain erc8004:AgentRegistration8004 ;
  rdfs:range core:Endpoint .

core:hasSkill a owl:ObjectProperty ;
  rdfs:domain erc8004:AgentRegistration8004 ;
  rdfs:range core:AgentSkill .

core:hasSkillClassification a owl:ObjectProperty ;
  rdfs:domain core:AgentSkill ;
  rdfs:range core:AgentSkillClassification .

core:hasDomain a owl:ObjectProperty ;
  rdfs:domain erc8004:AgentRegistration8004 ;
  rdfs:range core:AgentDomain .

core:hasDomainClassification a owl:ObjectProperty ;
  rdfs:domain core:AgentDomain ;
  rdfs:range core:AgentDomainClassification .


