@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix agentictrust: <https://www.agentictrust.io/ontology/agentictrust-core#> .
@prefix erc8004: <https://www.agentictrust.io/ontology/ERC8004#> .

erc8004:Diagram_Registration a owl:Ontology ;
  rdfs:label "ERC8004RegistrationDescriptor" .

prov:Entity a owl:Class .
prov:SoftwareAgent a owl:Class .

agentictrust:AIAgent a owl:Class ; rdfs:subClassOf prov:SoftwareAgent .

erc8004:AgentIdentity8004 a owl:Class ; rdfs:subClassOf prov:Entity .
erc8004:AgentRegistration8004 a owl:Class ; rdfs:subClassOf prov:Entity .

agentictrust:Endpoint a owl:Class ; rdfs:subClassOf prov:Entity .
agentictrust:AgentSkill a owl:Class ; rdfs:subClassOf prov:Entity .
agentictrust:AgentSkillClassification a owl:Class ; rdfs:subClassOf prov:Entity .
agentictrust:AgentDomain a owl:Class ; rdfs:subClassOf prov:Entity .
agentictrust:AgentDomainClassification a owl:Class ; rdfs:subClassOf prov:Entity .

agentictrust:hasIdentity a owl:ObjectProperty ;
  rdfs:domain agentictrust:AIAgent ;
  rdfs:range erc8004:AgentIdentity8004 .

agentictrust:hasDescriptor a owl:ObjectProperty ;
  rdfs:domain erc8004:AgentIdentity8004 ;
  rdfs:range erc8004:AgentRegistration8004 .

agentictrust:hasEndpoint a owl:ObjectProperty ;
  rdfs:domain erc8004:AgentRegistration8004 ;
  rdfs:range agentictrust:Endpoint .

agentictrust:hasSkill a owl:ObjectProperty ;
  rdfs:domain erc8004:AgentRegistration8004 ;
  rdfs:range agentictrust:AgentSkill .

agentictrust:hasSkillClassification a owl:ObjectProperty ;
  rdfs:domain agentictrust:AgentSkill ;
  rdfs:range agentictrust:AgentSkillClassification .

agentictrust:hasDomain a owl:ObjectProperty ;
  rdfs:domain erc8004:AgentRegistration8004 ;
  rdfs:range agentictrust:AgentDomain .

agentictrust:hasDomainClassification a owl:ObjectProperty ;
  rdfs:domain agentictrust:AgentDomain ;
  rdfs:range agentictrust:AgentDomainClassification .


