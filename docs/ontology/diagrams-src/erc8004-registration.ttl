@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix agentictrust: <https://www.agentictrust.io/ontology/agentictrust#> .
@prefix erc8004: <https://www.agentictrust.io/ontology/ERC8004#> .

erc8004:Diagram_Registration a owl:Ontology .

prov:Entity a owl:Class .
prov:SoftwareAgent a owl:Class .

agentictrust:AIAgent a owl:Class ; rdfs:subClassOf prov:SoftwareAgent .
agentictrust:AgentMetadata a owl:Class ; rdfs:subClassOf prov:Entity .
agentictrust:Skill a owl:Class ; rdfs:subClassOf prov:Entity .

erc8004:AgentRegistration a owl:Class ; rdfs:subClassOf agentictrust:AgentMetadata .
erc8004:ProtocolMetadata a owl:Class ; rdfs:subClassOf agentictrust:AgentMetadata .
erc8004:NameMetadata a owl:Class ; rdfs:subClassOf agentictrust:AgentMetadata .
erc8004:SkillsMetadata a owl:Class ; rdfs:subClassOf agentictrust:AgentMetadata .
erc8004:IntegrationMetadata a owl:Class ; rdfs:subClassOf agentictrust:AgentMetadata .

erc8004:hasAgentRegistration a owl:ObjectProperty ;
  rdfs:domain agentictrust:AIAgent ;
  rdfs:range erc8004:AgentRegistration .

erc8004:hasProtocolMetadata a owl:ObjectProperty ;
  rdfs:domain erc8004:AgentRegistration ;
  rdfs:range erc8004:ProtocolMetadata .

erc8004:hasNameMetadata a owl:ObjectProperty ;
  rdfs:domain erc8004:AgentRegistration ;
  rdfs:range erc8004:NameMetadata .

erc8004:hasSkillsMetadata a owl:ObjectProperty ;
  rdfs:domain erc8004:AgentRegistration ;
  rdfs:range erc8004:SkillsMetadata .

erc8004:hasIntegrationMetadata a owl:ObjectProperty ;
  rdfs:domain erc8004:AgentRegistration ;
  rdfs:range erc8004:IntegrationMetadata .

erc8004:exposesSkill a owl:ObjectProperty ;
  rdfs:domain erc8004:ProtocolMetadata ;
  rdfs:range agentictrust:Skill .

erc8004:listsSkill a owl:ObjectProperty ;
  rdfs:domain erc8004:SkillsMetadata ;
  rdfs:range agentictrust:Skill .


