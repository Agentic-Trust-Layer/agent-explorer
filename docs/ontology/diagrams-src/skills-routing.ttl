@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix core: <https://agentictrust.io/ontology/core#> .

core:Diagram_SkillsRouting a owl:Ontology .

prov:Entity a owl:Class .

core:AgentSkillClassification a owl:Class ; rdfs:subClassOf prov:Entity .
core:Intent a owl:Class ; rdfs:subClassOf prov:Entity .
core:IntentType a owl:Class ; rdfs:subClassOf prov:Entity .
core:IntentSubject a owl:Class ; rdfs:subClassOf prov:Entity .
core:IntentCheck a owl:Class ; rdfs:subClassOf prov:Entity .
core:TaskType a owl:Class ; rdfs:subClassOf prov:Entity .

core:hasSubject a owl:ObjectProperty ;
  rdfs:domain core:Intent ;
  rdfs:range core:IntentSubject .

core:hasCheck a owl:ObjectProperty ;
  rdfs:domain core:Intent ;
  rdfs:range core:IntentCheck .

core:supportsIntentType a owl:ObjectProperty ;
  rdfs:domain core:AgentSkillClassification ;
  rdfs:range core:IntentType .

core:mapsToTaskType a owl:ObjectProperty ;
  rdfs:domain core:IntentType ;
  rdfs:range core:TaskType .

core:enablesTaskType a owl:ObjectProperty ;
  rdfs:domain core:AgentSkillClassification ;
  rdfs:range core:TaskType .

core:implementedBySkill a owl:ObjectProperty ;
  rdfs:domain core:TaskType ;
  rdfs:range core:AgentSkillClassification .


