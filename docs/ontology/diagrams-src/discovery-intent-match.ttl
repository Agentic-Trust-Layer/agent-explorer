@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix core: <https://agentictrust.io/ontology/core#> .

core:Diagram_DiscoveryIntentMatch a owl:Ontology ;
  rdfs:label "DiscoveryIntentMatch" .

core:Message a owl:Class .
core:Intent a owl:Class .
core:IntentType a owl:Class .
core:IntentSubject a owl:Class .
core:IntentCheck a owl:Class .

core:AgentSkillClassification a owl:Class .
core:TaskType a owl:Class .

core:hasIntent a owl:ObjectProperty ;
  rdfs:domain core:Message ;
  rdfs:range core:Intent .

core:hasSubject a owl:ObjectProperty ;
  rdfs:domain core:Intent ;
  rdfs:range core:IntentSubject .

core:hasCheck a owl:ObjectProperty ;
  rdfs:domain core:Intent ;
  rdfs:range core:IntentCheck .

core:supportsIntentType a owl:ObjectProperty ;
  rdfs:domain core:AgentSkillClassification ;
  rdfs:range core:IntentType .

core:mapsToTaskType a owl:ObjectProperty ;
  rdfs:domain core:IntentType ;
  rdfs:range core:TaskType .

core:enablesTaskType a owl:ObjectProperty ;
  rdfs:domain core:AgentSkillClassification ;
  rdfs:range core:TaskType .


