@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix agentictrust: <https://www.agentictrust.io/ontology/agentictrust#> .

agentictrust:Diagram_DescriptionToDiscovery a owl:Ontology ;
  rdfs:label "DescriptionToDiscovery" .

agentictrust:TrustDescription a owl:Class .

agentictrust:AgentCardFetch a owl:Class .
agentictrust:AgentCard a owl:Class .
agentictrust:AgentMetadata a owl:Class .
agentictrust:Skill a owl:Class .

agentictrust:hadPlan a owl:ObjectProperty ;
  rdfs:domain agentictrust:AgentCardFetch ;
  rdfs:range agentictrust:TrustDescription .

agentictrust:generatedAssertion a owl:ObjectProperty ;
  rdfs:domain agentictrust:AgentCardFetch ;
  rdfs:range agentictrust:AgentCard .

agentictrust:hasSkill a owl:ObjectProperty ;
  rdfs:domain agentictrust:AgentCard ;
  rdfs:range agentictrust:Skill .

agentictrust:declaresSkill a owl:ObjectProperty ;
  rdfs:domain agentictrust:AgentMetadata ;
  rdfs:range agentictrust:Skill .


