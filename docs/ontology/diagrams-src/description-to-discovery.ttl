@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix core: <https://agentictrust.io/ontology/core#> .

core:Diagram_DescriptionToDiscovery a owl:Ontology ;
  rdfs:label "DescriptionToDiscovery" .

core:TrustDescription a owl:Class .

core:AgentCardFetch a owl:Class .
core:AgentDescriptor a owl:Class .
core:AgentSkill a owl:Class .
core:AgentSkillClassification a owl:Class .

core:hadPlan a owl:ObjectProperty ;
  rdfs:domain core:AgentCardFetch ;
  rdfs:range core:TrustDescription .

prov:generated a owl:ObjectProperty ;
  rdfs:domain core:AgentCardFetch ;
  rdfs:range core:AgentDescriptor .

core:hasSkill a owl:ObjectProperty ;
  rdfs:domain core:AgentDescriptor ;
  rdfs:range core:AgentSkill .

core:hasSkillClassification a owl:ObjectProperty ;
  rdfs:domain core:AgentSkill ;
  rdfs:range core:AgentSkillClassification .


