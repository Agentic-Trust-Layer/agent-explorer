@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix agentictrust: <https://www.agentictrust.io/ontology/agentictrust-core#> .
@prefix agentictrustEth: <https://www.agentictrust.io/ontology/agentictrust-eth#> .
@prefix erc8092: <https://www.agentictrust.io/ontology/ERC8092#> .

erc8092:Diagram_Relationships a owl:Ontology .

prov:Entity a owl:Class .
prov:Activity a owl:Class .
prov:Agent a owl:Class .

agentictrust:TrustAssertion a owl:Class ; rdfs:subClassOf prov:Entity .
agentictrust:RelationshipTrustAssertion a owl:Class ; rdfs:subClassOf agentictrust:TrustAssertion .
agentictrust:TrustAssertionAct a owl:Class ; rdfs:subClassOf prov:Activity .

agentictrust:Situation a owl:Class ; rdfs:subClassOf prov:Entity .
agentictrust:RelationshipSituation a owl:Class ; rdfs:subClassOf agentictrust:Situation .
agentictrust:TrustSituation a owl:Class ; rdfs:subClassOf agentictrust:Situation .
agentictrust:RelationshipTrustSituation a owl:Class ; 
  rdfs:subClassOf agentictrust:RelationshipSituation ;
  rdfs:subClassOf agentictrust:TrustSituation .

erc8092:TrustAssertion a owl:Class ; rdfs:subClassOf agentictrust:TrustAssertion .
erc8092:TrustAssertionAct a owl:Class ; rdfs:subClassOf agentictrust:TrustAssertionAct .

erc8092:AssociatedAccounts8092 a owl:Class ; 
  rdfs:subClassOf erc8092:TrustAssertion ;
  rdfs:subClassOf agentictrust:RelationshipTrustAssertion .
erc8092:AssociatedAccountsAct8092 a owl:Class ; rdfs:subClassOf erc8092:TrustAssertionAct .
erc8092:AssociatedAccountsRevocation8092 a owl:Class ; 
  rdfs:subClassOf erc8092:TrustAssertion ;
  rdfs:subClassOf agentictrust:RelationshipTrustAssertion .
erc8092:AssociatedAccountsRevocationAct8092 a owl:Class ; rdfs:subClassOf erc8092:TrustAssertionAct .

agentictrustEth:Account a owl:Class ; rdfs:subClassOf prov:Agent .
agentictrust:AIAgent a owl:Class ; rdfs:subClassOf prov:Agent .

agentictrust:assertsSituation a owl:ObjectProperty ;
  rdfs:domain prov:Activity ;
  rdfs:range prov:Entity .

agentictrust:recordsSituation a owl:ObjectProperty ;
  rdfs:domain prov:Entity ;
  rdfs:range prov:Entity .

agentictrust:generatedAssertionRecord a owl:ObjectProperty ;
  rdfs:domain prov:Activity ;
  rdfs:range prov:Entity .

agentictrust:isAboutAgent a owl:ObjectProperty ;
  rdfs:domain prov:Entity ;
  rdfs:range prov:Agent .

agentictrust:hasSituationParticipant a owl:ObjectProperty ;
  rdfs:domain prov:Entity ;
  rdfs:range prov:Agent .

erc8092:hasAssociatedAccounts a owl:ObjectProperty ;
  rdfs:domain agentictrustEth:Account ;
  rdfs:range erc8092:AssociatedAccounts8092 .

erc8092:initiator a owl:ObjectProperty ;
  rdfs:domain erc8092:AssociatedAccounts8092 ;
  rdfs:range agentictrustEth:Account .

erc8092:approver a owl:ObjectProperty ;
  rdfs:domain erc8092:AssociatedAccounts8092 ;
  rdfs:range agentictrustEth:Account .

erc8092:revocationOfAssociatedAccounts a owl:ObjectProperty ;
  rdfs:domain erc8092:AssociatedAccountsRevocation8092 ;
  rdfs:range erc8092:TrustAssertion .


