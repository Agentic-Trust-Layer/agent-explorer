@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix core: <https://agentictrust.io/ontology/core#> .
@prefix eth: <https://agentictrust.io/ontology/eth#> .
@prefix erc8092: <https://agentictrust.io/ontology/erc8092#> .

erc8092:Diagram_Relationships a owl:Ontology .

prov:Entity a owl:Class .
prov:Activity a owl:Class .
prov:Agent a owl:Class .

core:TrustAssertion a owl:Class ; rdfs:subClassOf prov:Entity .
core:RelationshipTrustAssertion a owl:Class ; rdfs:subClassOf core:TrustAssertion .
core:TrustAssertionAct a owl:Class ; rdfs:subClassOf prov:Activity .

core:Situation a owl:Class ; rdfs:subClassOf prov:Entity .
core:RelationshipSituation a owl:Class ; rdfs:subClassOf core:Situation .
core:TrustSituation a owl:Class ; rdfs:subClassOf core:Situation .
core:RelationshipTrustSituation a owl:Class ; 
  rdfs:subClassOf core:RelationshipSituation ;
  rdfs:subClassOf core:TrustSituation .

erc8092:TrustAssertion a owl:Class ; rdfs:subClassOf core:TrustAssertion .
erc8092:TrustAssertionAct a owl:Class ; rdfs:subClassOf core:TrustAssertionAct .

erc8092:AssociatedAccounts8092 a owl:Class ; 
  rdfs:subClassOf erc8092:TrustAssertion ;
  rdfs:subClassOf core:RelationshipTrustAssertion .
erc8092:AssociatedAccountsAct8092 a owl:Class ; rdfs:subClassOf erc8092:TrustAssertionAct .
erc8092:AssociatedAccountsRevocation8092 a owl:Class ; 
  rdfs:subClassOf erc8092:TrustAssertion ;
  rdfs:subClassOf core:RelationshipTrustAssertion .
erc8092:AssociatedAccountsRevocationAct8092 a owl:Class ; rdfs:subClassOf erc8092:TrustAssertionAct .

eth:Account a owl:Class ; rdfs:subClassOf prov:Agent .
core:AIAgent a owl:Class ; rdfs:subClassOf prov:Agent .

core:assertsSituation a owl:ObjectProperty ;
  rdfs:domain prov:Activity ;
  rdfs:range prov:Entity .

core:recordsSituation a owl:ObjectProperty ;
  rdfs:domain prov:Entity ;
  rdfs:range prov:Entity .

core:generatedAssertionRecord a owl:ObjectProperty ;
  rdfs:domain prov:Activity ;
  rdfs:range prov:Entity .

core:isAboutAgent a owl:ObjectProperty ;
  rdfs:domain prov:Entity ;
  rdfs:range prov:Agent .

core:hasSituationParticipant a owl:ObjectProperty ;
  rdfs:domain prov:Entity ;
  rdfs:range prov:Agent .

erc8092:hasAssociatedAccounts a owl:ObjectProperty ;
  rdfs:domain eth:Account ;
  rdfs:range erc8092:AssociatedAccounts8092 .

erc8092:initiator a owl:ObjectProperty ;
  rdfs:domain erc8092:AssociatedAccounts8092 ;
  rdfs:range eth:Account .

erc8092:approver a owl:ObjectProperty ;
  rdfs:domain erc8092:AssociatedAccounts8092 ;
  rdfs:range eth:Account .

erc8092:revocationOfAssociatedAccounts a owl:ObjectProperty ;
  rdfs:domain erc8092:AssociatedAccountsRevocation8092 ;
  rdfs:range erc8092:TrustAssertion .


