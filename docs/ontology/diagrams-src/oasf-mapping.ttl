@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix core: <https://agentictrust.io/ontology/core#> .

core:Diagram_OASFMapping a owl:Ontology ;
  rdfs:label "OASFtoAgenticTrustMapping" .

### OASF concepts (modeled as classes purely for diagramming)
core:OASFRecord a owl:Class ; rdfs:label "OASFRecord" .
core:OASFSkill a owl:Class ; rdfs:label "OASFSkill" .
core:OASFDomain a owl:Class ; rdfs:label "OASFDomain" .
core:OASFModule a owl:Class ; rdfs:label "OASFModule" .

core:recordsAnnotatedWithSkill a owl:ObjectProperty ;
  rdfs:label "annotatedWithSkill" ;
  rdfs:domain core:OASFRecord ;
  rdfs:range core:OASFSkill .

core:recordsAnnotatedWithDomain a owl:ObjectProperty ;
  rdfs:label "annotatedWithDomain" ;
  rdfs:domain core:OASFRecord ;
  rdfs:range core:OASFDomain .

core:recordsHasModule a owl:ObjectProperty ;
  rdfs:label "hasModule" ;
  rdfs:domain core:OASFRecord ;
  rdfs:range core:OASFModule .

### AgenticTrust concepts (actual ontology nodes)
core:AIAgent a owl:Class .
core:AgentDescriptor a owl:Class .
core:AgentSkill a owl:Class .
core:AgentSkillClassification a owl:Class .
core:Tag a owl:Class .
core:IntentType a owl:Class .
core:TaskType a owl:Class .
core:JsonSchema a owl:Class .

core:hasAgentDescriptor a owl:ObjectProperty ;
  rdfs:domain core:AIAgent ;
  rdfs:range core:AgentDescriptor .

core:hasSkill a owl:ObjectProperty ;
  rdfs:domain core:AgentDescriptor ;
  rdfs:range core:AgentSkill .

core:hasSkillClassification a owl:ObjectProperty ;
  rdfs:domain core:AgentSkill ;
  rdfs:range core:AgentSkillClassification .

core:hasTag a owl:ObjectProperty ;
  rdfs:domain core:AgentSkillClassification ;
  rdfs:range core:Tag .

core:supportsIntentType a owl:ObjectProperty ;
  rdfs:domain core:AgentSkillClassification ;
  rdfs:range core:IntentType .

core:mapsToTaskType a owl:ObjectProperty ;
  rdfs:domain core:IntentType ;
  rdfs:range core:TaskType .

core:hasInputSchema a owl:ObjectProperty ;
  rdfs:domain core:AgentSkillClassification ;
  rdfs:range core:JsonSchema .

core:hasOutputSchema a owl:ObjectProperty ;
  rdfs:domain core:AgentSkillClassification ;
  rdfs:range core:JsonSchema .

### Mapping edges (diagram overlay)
core:mapsToAgentCard a owl:ObjectProperty ;
  rdfs:label "mapsTo" ;
  rdfs:domain core:OASFRecord ;
  rdfs:range core:AgentDescriptor .

core:mapsToSkill a owl:ObjectProperty ;
  rdfs:label "mapsTo" ;
  rdfs:domain core:OASFSkill ;
  rdfs:range core:AgentSkillClassification .

core:mapsToTag a owl:ObjectProperty ;
  rdfs:label "mapsTo" ;
  rdfs:domain core:OASFDomain ;
  rdfs:range core:Tag .

core:mapsToIntentType a owl:ObjectProperty ;
  rdfs:label "mapsTo" ;
  rdfs:domain core:OASFDomain ;
  rdfs:range core:IntentType .

core:mapsToSchema a owl:ObjectProperty ;
  rdfs:label "mapsTo" ;
  rdfs:domain core:OASFModule ;
  rdfs:range core:JsonSchema .


