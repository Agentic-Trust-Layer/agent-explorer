@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix p-plan: <http://purl.org/net/p-plan#> .
@prefix core: <https://agentictrust.io/ontology/core#> .
@prefix erc8004: <https://agentictrust.io/ontology/erc8004#> .
@prefix erc8092: <https://agentictrust.io/ontology/erc8092#> .

core:Diagram_OverviewStack a owl:Ontology ;
  dcterms:title "AgenticTrustOntologyOverviewStack" ;
  rdfs:label "AgenticTrustOntologyOverviewStack" .

### “Layer” nodes (modeled as classes for diagram purposes)
core:UpperOntologyPatterns a owl:Class ;
  rdfs:label "UpperOntologyPatterns" .

prov:PROVO a owl:Class ;
  rdfs:label "PROV-O" ;
  rdfs:subClassOf core:UpperOntologyPatterns .

p-plan:PPLAN a owl:Class ;
  rdfs:label "P-PLAN" ;
  rdfs:subClassOf core:UpperOntologyPatterns .

core:DnSPrinciples a owl:Class ;
  rdfs:label "DnSPrinciples" ;
  rdfs:subClassOf core:UpperOntologyPatterns .

core:DOLCEPrinciples a owl:Class ;
  rdfs:label "DOLCEPrinciples" ;
  rdfs:subClassOf core:UpperOntologyPatterns .

core:AgenticTrustCore a owl:Class ;
  rdfs:label "AgenticTrustCoreOntology" ;
  rdfs:subClassOf prov:PROVO, p-plan:PPLAN, core:DnSPrinciples, core:DOLCEPrinciples .

core:ERC8004 a owl:Class ;
  rdfs:label "ERC8004" ;
  rdfs:subClassOf core:AgenticTrustCore .

core:NANDA a owl:Class ;
  rdfs:label "NANDA" ;
  rdfs:subClassOf core:AgenticTrustCore .

core:AGNTCY a owl:Class ;
  rdfs:label "AGNTCY" ;
  rdfs:subClassOf core:AgenticTrustCore .

core:ANS a owl:Class ;
  rdfs:label "ANS" ;
  rdfs:subClassOf core:AgenticTrustCore .



