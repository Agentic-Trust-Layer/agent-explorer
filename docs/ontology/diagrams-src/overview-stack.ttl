@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix p-plan: <http://purl.org/net/p-plan#> .
@prefix agentictrust: <https://www.agentictrust.io/ontology/agentictrust#> .
@prefix erc8004: <https://www.agentictrust.io/ontology/ERC8004#> .
@prefix erc8092: <https://www.agentictrust.io/ontology/ERC8092#> .

agentictrust:Diagram_OverviewStack a owl:Ontology .

### “Layer” nodes (modeled as classes for diagram purposes)
agentictrust:UpperOntologyPatterns a owl:Class ;
  rdfs:label "UpperOntologyPatterns" .

prov:PROVO a owl:Class ;
  rdfs:label "PROV-O" ;
  rdfs:subClassOf agentictrust:UpperOntologyPatterns .

p-plan:PPLAN a owl:Class ;
  rdfs:label "P-PLAN" ;
  rdfs:subClassOf agentictrust:UpperOntologyPatterns .

agentictrust:DnSPrinciples a owl:Class ;
  rdfs:label "DnSPrinciples" ;
  rdfs:subClassOf agentictrust:UpperOntologyPatterns .

agentictrust:DOLCEPrinciples a owl:Class ;
  rdfs:label "DOLCEPrinciples" ;
  rdfs:subClassOf agentictrust:UpperOntologyPatterns .

agentictrust:AgenticTrustCore a owl:Class ;
  rdfs:label "AgenticTrustCoreOntology" ;
  rdfs:subClassOf prov:PROVO, p-plan:PPLAN, agentictrust:DnSPrinciples, agentictrust:DOLCEPrinciples .

erc8004:ERC8004RegistryLayer a owl:Class ;
  rdfs:label "ERC8004RegistryLayer" ;
  rdfs:subClassOf agentictrust:AgenticTrustCore .

erc8092:ERC8092RegistryLayer a owl:Class ;
  rdfs:label "ERC8092RegistryLayer" ;
  rdfs:subClassOf agentictrust:AgenticTrustCore .

agentictrust:ProtocolLayer a owl:Class ;
  rdfs:label "ProtocolLayers(A2A/MCP/etc)" ;
  rdfs:subClassOf agentictrust:AgenticTrustCore .

agentictrust:AgenticTrustKnowledgeBase a owl:Class ;
  rdfs:label "AgenticTrustKnowledgeBase" .

agentictrust:populatesKnowledgeBase a owl:ObjectProperty ;
  rdfs:label "populatesKnowledgeBase" ;
  rdfs:domain agentictrust:AgenticTrustCore ;
  rdfs:range agentictrust:AgenticTrustKnowledgeBase .

erc8004:populatesKnowledgeBase a owl:ObjectProperty ;
  rdfs:label "populatesKnowledgeBase" ;
  rdfs:domain erc8004:ERC8004RegistryLayer ;
  rdfs:range agentictrust:AgenticTrustKnowledgeBase .

erc8092:populatesKnowledgeBase a owl:ObjectProperty ;
  rdfs:label "populatesKnowledgeBase" ;
  rdfs:domain erc8092:ERC8092RegistryLayer ;
  rdfs:range agentictrust:AgenticTrustKnowledgeBase .


