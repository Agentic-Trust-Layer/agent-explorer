@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix core: <https://agentictrust.io/ontology/core#> .

core:Diagram_SkillsExecution a owl:Ontology .

prov:Activity a owl:Class .
prov:Entity a owl:Class .

core:Task a owl:Class ; rdfs:subClassOf prov:Entity .
core:TaskExecution a owl:Class ; rdfs:subClassOf prov:Activity .
core:SkillInvocation a owl:Class ; rdfs:subClassOf prov:Activity .
core:AgentSkillClassification a owl:Class ; rdfs:subClassOf prov:Entity .
core:Message a owl:Class ; rdfs:subClassOf prov:Entity .
core:Intent a owl:Class ; rdfs:subClassOf prov:Entity .

core:taskRealizedBy a owl:ObjectProperty ;
  rdfs:domain core:Task ;
  rdfs:range core:TaskExecution .

core:hasInvocation a owl:ObjectProperty ;
  rdfs:domain core:TaskExecution ;
  rdfs:range core:SkillInvocation .

core:invokesSkill a owl:ObjectProperty ;
  rdfs:domain core:SkillInvocation ;
  rdfs:range core:AgentSkillClassification .

core:invocationUsedMessage a owl:ObjectProperty ;
  rdfs:domain core:SkillInvocation ;
  rdfs:range core:Message .

core:fulfilledByInvocation a owl:ObjectProperty ;
  rdfs:domain core:Intent ;
  rdfs:range core:SkillInvocation .


