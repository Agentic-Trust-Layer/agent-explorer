@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix agentictrust: <https://www.agentictrust.io/ontology/agentictrust#> .

agentictrust:Diagram_SkillsExecution a owl:Ontology .

prov:Activity a owl:Class .
prov:Entity a owl:Class .

agentictrust:Task a owl:Class ; rdfs:subClassOf prov:Entity .
agentictrust:TaskExecution a owl:Class ; rdfs:subClassOf prov:Activity .
agentictrust:SkillInvocation a owl:Class ; rdfs:subClassOf prov:Activity .
agentictrust:Skill a owl:Class ; rdfs:subClassOf prov:Entity .
agentictrust:Message a owl:Class ; rdfs:subClassOf prov:Entity .
agentictrust:Intent a owl:Class ; rdfs:subClassOf prov:Entity .

agentictrust:taskRealizedBy a owl:ObjectProperty ;
  rdfs:domain agentictrust:Task ;
  rdfs:range agentictrust:TaskExecution .

agentictrust:hasInvocation a owl:ObjectProperty ;
  rdfs:domain agentictrust:TaskExecution ;
  rdfs:range agentictrust:SkillInvocation .

agentictrust:invokesSkill a owl:ObjectProperty ;
  rdfs:domain agentictrust:SkillInvocation ;
  rdfs:range agentictrust:Skill .

agentictrust:invocationUsedMessage a owl:ObjectProperty ;
  rdfs:domain agentictrust:SkillInvocation ;
  rdfs:range agentictrust:Message .

agentictrust:fulfilledByInvocation a owl:ObjectProperty ;
  rdfs:domain agentictrust:Intent ;
  rdfs:range agentictrust:SkillInvocation .


