@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix core: <https://agentictrust.io/ontology/core#> .
@prefix erc8004: <https://agentictrust.io/ontology/erc8004#> .

erc8004:Diagram_Trust a owl:Ontology .

prov:Activity a owl:Class .
prov:Entity a owl:Class .
prov:SoftwareAgent a owl:Class .

core:AIAgent a owl:Class ; rdfs:subClassOf prov:SoftwareAgent .
core:TrustSituation a owl:Class ; rdfs:subClassOf prov:Entity .
core:TrustAssertion a owl:Class ; rdfs:subClassOf prov:Entity .
core:VerificationTrustAssertion a owl:Class ; rdfs:subClassOf core:TrustAssertion .
core:ReputationTrustAssertion a owl:Class ; rdfs:subClassOf core:TrustAssertion .

erc8004:TrustAssertion a owl:Class ; rdfs:subClassOf core:TrustAssertion .

core:VerificationRequestSituation a owl:Class ; rdfs:subClassOf core:TrustSituation .
erc8004:ValidationResponse a owl:Class ;
  rdfs:subClassOf erc8004:TrustAssertion, core:VerificationTrustAssertion .

erc8004:Feedback a owl:Class ;
  rdfs:subClassOf erc8004:TrustAssertion, core:ReputationTrustAssertion .

erc8004:FeedbackResponse a owl:Class ;
  rdfs:subClassOf erc8004:TrustAssertion, core:ReputationTrustAssertion .

erc8004:hasFeedback a owl:ObjectProperty ;
  rdfs:domain core:AIAgent ;
  rdfs:range erc8004:Feedback .

erc8004:hasValidation a owl:ObjectProperty ;
  rdfs:domain core:AIAgent ;
  rdfs:range erc8004:ValidationResponse .

erc8004:validationRespondsToRequest a owl:ObjectProperty ;
  rdfs:domain erc8004:ValidationResponse ;
  rdfs:range core:VerificationRequestSituation .


