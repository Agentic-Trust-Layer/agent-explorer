@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix core: <https://agentictrust.io/ontology/core#> .

core:Diagram_SkillsDeclarations a owl:Ontology .

prov:Entity a owl:Class .

core:AgentDescriptor a owl:Class ; rdfs:subClassOf prov:Entity .
core:AgentSkill a owl:Class ; rdfs:subClassOf prov:Entity .
core:AgentSkillClassification a owl:Class ; rdfs:subClassOf prov:Entity .
core:Tag a owl:Class ; rdfs:subClassOf prov:Entity .
core:SkillExample a owl:Class ; rdfs:subClassOf prov:Entity .
core:JsonSchema a owl:Class ; rdfs:subClassOf prov:Entity .
core:IntentType a owl:Class ; rdfs:subClassOf prov:Entity .

core:hasSkill a owl:ObjectProperty ;
  rdfs:domain core:AgentDescriptor ;
  rdfs:range core:AgentSkill .

core:hasSkillClassification a owl:ObjectProperty ;
  rdfs:domain core:AgentSkill ;
  rdfs:range core:AgentSkillClassification .

core:hasTag a owl:ObjectProperty ;
  rdfs:domain core:AgentSkillClassification ;
  rdfs:range core:Tag .

core:hasExample a owl:ObjectProperty ;
  rdfs:domain core:AgentSkillClassification ;
  rdfs:range core:SkillExample .

core:hasInputSchema a owl:ObjectProperty ;
  rdfs:domain core:AgentSkillClassification ;
  rdfs:range core:JsonSchema .

core:hasOutputSchema a owl:ObjectProperty ;
  rdfs:domain core:AgentSkillClassification ;
  rdfs:range core:JsonSchema .

core:supportsIntentType a owl:ObjectProperty ;
  rdfs:domain core:AgentSkillClassification ;
  rdfs:range core:IntentType .


