@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix agentictrust: <https://www.agentictrust.io/ontology/agentictrust-core#> .

agentictrust:Diagram_CoreMetadata a owl:Ontology .

prov:SoftwareAgent a owl:Class .
prov:Entity a owl:Class .

agentictrust:AIAgent a owl:Class ;
  rdfs:subClassOf prov:SoftwareAgent .

agentictrust:AgentDescriptor a owl:Class ;
  rdfs:subClassOf prov:Entity .

agentictrust:EndpointType a owl:Class ;
  rdfs:subClassOf prov:Entity .

agentictrust:Endpoint a owl:Class ;
  rdfs:subClassOf prov:Entity .

agentictrust:AgentSkill a owl:Class ;
  rdfs:subClassOf prov:Entity .

agentictrust:AgentSkillClassification a owl:Class ;
  rdfs:subClassOf prov:Entity .

agentictrust:hasAgentDescriptor a owl:ObjectProperty ;
  rdfs:domain agentictrust:AIAgent ;
  rdfs:range agentictrust:AgentDescriptor .

agentictrust:assembledFromMetadata a owl:ObjectProperty ;
  rdfs:domain agentictrust:AgentDescriptor ;
  rdfs:range agentictrust:AgentDescriptor .

agentictrust:hasEndpoint a owl:ObjectProperty ;
  rdfs:domain agentictrust:AgentDescriptor ;
  rdfs:range agentictrust:Endpoint .

agentictrust:endpointType a owl:ObjectProperty ;
  rdfs:domain agentictrust:Endpoint ;
  rdfs:range agentictrust:EndpointType .

agentictrust:hasSkill a owl:ObjectProperty ;
  rdfs:domain agentictrust:AgentDescriptor ;
  rdfs:range agentictrust:AgentSkill .

agentictrust:hasSkillClassification a owl:ObjectProperty ;
  rdfs:domain agentictrust:AgentSkill ;
  rdfs:range agentictrust:AgentSkillClassification .


