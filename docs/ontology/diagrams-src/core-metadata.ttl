@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix core: <https://agentictrust.io/ontology/core#> .

core:Diagram_CoreMetadata a owl:Ontology .

prov:SoftwareAgent a owl:Class .
prov:Entity a owl:Class .

core:AIAgent a owl:Class ;
  rdfs:subClassOf prov:SoftwareAgent .

core:AgentDescriptor a owl:Class ;
  rdfs:subClassOf prov:Entity .

core:EndpointType a owl:Class ;
  rdfs:subClassOf prov:Entity .

core:Endpoint a owl:Class ;
  rdfs:subClassOf prov:Entity .

core:ServiceEndpoint a owl:Class ;
  rdfs:subClassOf core:Endpoint .

core:Protocol a owl:Class ;
  rdfs:subClassOf prov:Entity .

core:AgentSkill a owl:Class ;
  rdfs:subClassOf prov:Entity .

core:AgentSkillClassification a owl:Class ;
  rdfs:subClassOf prov:Entity .

core:hasAgentDescriptor a owl:ObjectProperty ;
  rdfs:domain core:AIAgent ;
  rdfs:range core:AgentDescriptor .

core:hasServiceEndpoint a owl:ObjectProperty ;
  rdfs:domain prov:Entity ;
  rdfs:range core:ServiceEndpoint .

core:hasProtocol a owl:ObjectProperty ;
  rdfs:domain core:ServiceEndpoint ;
  rdfs:range core:Protocol .

core:serviceUrl a owl:ObjectProperty ;
  rdfs:domain core:ServiceEndpoint ;
  rdfs:range owl:Thing .

core:hasEndpoint a owl:ObjectProperty ;
  rdfs:domain core:AgentDescriptor ;
  rdfs:range core:Endpoint .

core:endpointType a owl:ObjectProperty ;
  rdfs:domain core:Endpoint ;
  rdfs:range core:EndpointType .

core:hasSkill a owl:ObjectProperty ;
  rdfs:domain core:AgentDescriptor ;
  rdfs:range core:AgentSkill .

core:hasSkillClassification a owl:ObjectProperty ;
  rdfs:domain core:AgentSkill ;
  rdfs:range core:AgentSkillClassification .


